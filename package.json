{
  "name": "project-name",
  "version": "0.0.0",
  "description": "A description for the project",
  "main": "app.js",
  "engines": {
    "node": "6.2.0",
    "npm": "3.8.9"
  },
  "scripts": {
    "prescss": "mkdirp public/css",
    "scss": "node-sass --output-style compressed -o public/css src/scss",
    "autoprefixer": "postcss -u autoprefixer -r public/css/*",
    "build:css": "npm run scss && npm run autoprefixer",
    "prebuild:js": "mkdirp public/js",
    "build:js": "webpack",
    "preimagemin": "mkdirp public/img",
    "imagemin": "imagemin src/img/* --out-dir=public/img",
    "prefonts": "mkdirp public/fonts/",
    "fonts": "cp src/fonts/* public/fonts/",
    "build": "npm run build:css && npm run build:js && npm run imagemin && npm run fonts",
    "tape": "babel-tape-runner test/unit/*",
    "nightwatch": "npm start & nightwatch",
    "test": "npm run tape && npm run nightwatch",
    "start": "mkdirp logs && node ./bin/www.js",
    "rebuild": "npm run build && npm start",
    "rebuild:js": "npm run build:js && npm start",
    "rebuild:css": "npm run build:css && npm start",
    "postinstall": "npm run build",
    "clean": "rimraf pulic logs nwreports selenium-debug.log",
    "clean:all": "npm run clean && rimraf npm_modules",
    "preversion": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sergionsz/expresstemplate"
  },
  "keywords": [
    "all",
    "your",
    "keywords"
  ],
  "author": "Sergio NS",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/sergionsz/expresstemplate/issues"
  },
  "devDependencies": {
    "babel-tape-runner": "^2.0.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "jquery": "^3.1.0",
    "jsdom": "^9.4.2",
    "jsdom-global": "^2.1.0",
    "nightwatch": "^0.9.8",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "autoprefixer": "6.3.6",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "6.9.0",
    "debug": "2.2.0",
    "dotenv": "2.0.0",
    "express": "4.14.0",
    "imagemin-cli": "3.0.0",
    "mkdirp": "0.5.1",
    "node-sass": "3.8.0",
    "postcss-cli": "2.5.2",
    "pug": "2.0.0-beta3",
    "tape": "4.6.0",
    "webpack": "^1.13.2",
    "winston": "2.2.0"
  }
}
